{
  "name": "apkup",
  "version": "1.1.0",
  "description": "Publish APKs to Google Play Store",
  "main": "lib/index.js",
  "bin": {
    "apkup": "cli.js"
  },
  "scripts": {
    "prepublishOnly": "npm run build && npm prune",
    "build": "babel src --out-dir lib",
    "build-debug": "babel src --out-dir lib --source-maps inline",
    "watch": "babel src --watch --out-dir lib --source-maps true",
    "docs": "jsdoc src/ README.md",
    "test": "eslint . && tap test/*.js",
    "test-lcov": "eslint . &&  tap --coverage-report=lcov --coverage test/*.js",
    "test:audit": "npm audit --json | npm-audit-html --output reports/audit.html && npm audit"
  },
  "repository": "Filiosoft/apkup",
  "keywords": [
    "android",
    "google",
    "play",
    "google play",
    "mobile",
    "apk"
  ],
  "author": "Filiosoft Labs <opensource@filiosoft.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Filiosoft/apkup/issues"
  },
  "homepage": "https://github.com/Filiosoft/apkup",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "adbkit-apkreader": "^3.1.1",
    "debug": "^2.1.3",
    "googleapis": "^36.0.0",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jsdoc": "^3.5.5",
    "npm-audit-html": "^1.2.0",
    "proxyquire": "^1.6.0",
    "sinon": "^1.15.4",
    "tap": "^1.3.1",
    "tape": "^4.0.0"
  }
}
