{
  "name": "apkup",
  "version": "1.1.0",
  "description": "Publish APKs to Google Play Store",
  "main": "lib/index.js",
  "bin": {
    "apkup": "cli.js"
  },
  "scripts": {
    "prepublish": "npm run build && npm prune",
    "build": "babel src --modules common --out-dir lib",
    "build-debug": "babel src --modules common --out-dir lib --source-maps inline",
    "watch": "babel src --watch --modules common --out-dir lib --source-maps true",
    "test": "eslint . && tap test/*.js",
    "test-lcov": "eslint . &&  tap --coverage-report=lcov --coverage test/*.js",
    "test:audit": "npm audit --json | npm-audit-html --output reports/audit.html && npm audit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Filiosoft/apkup"
  },
  "keywords": [
    "android",
    "google",
    "play",
    "google play",
    "mobile",
    "apk"
  ],
  "author": "Jeduan Cornejo <jeduan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Filiosoft/apkup/issues"
  },
  "homepage": "https://github.com/Filiosoft/apkup",
  "dependencies": {
    "debug": "^2.1.3",
    "googleapis": "^2.0.2",
    "node-apk-parser": "^0.2.3",
    "nodeify": "^1.0.0",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "npm-audit-html": "^1.2.0",
    "proxyquire": "^1.6.0",
    "sinon": "^1.15.4",
    "tap": "^1.3.1",
    "tape": "^4.0.0"
  }
}
